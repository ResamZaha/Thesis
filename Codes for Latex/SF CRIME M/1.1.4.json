{
    "IncidntNum": 1,
    "location": 1,
    "Time": 1,
    "Resolution": 1,
    "X": 1,
    "Y": 1,
    "location_type": {
      "$switch": {
        "branches": [
          { "case": { "$regexMatch": { "input": "$location", "regex": ".*STREET.*" } }, "then": "Street" },
          { "case": { "$regexMatch": { "input": "$location", "regex": ".*AVENUE.*" } }, "then": "Avenue" }
        ],
        "default": "Other"
      }
    }
}
  